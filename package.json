{
  "name": "book-organizer",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "dev:client": "vite --port 5173 --strictPort",
    "dev:server": "node server/index.mjs",
    "dev:full": "concurrently -k \"node server/index.mjs\" \"vite --port 5173 --strictPort\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 4173 --strictPort"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^11.0.0",
    "helmet": "^8.1.0",
    "lucide-react": "^0.446.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^4.0.18"
  }
}
